source ~/.dotfiles/vim/plugins.vim

" ----- key remappings -----
let mapleader=";"
source $HOME/.dotfiles/vim/keybindings.vim

" ----- General (non-plugin) settings -----
syntax on
set number
set backspace=indent,eol,start
set laststatus=2
set tabstop=4
highlight LineNr ctermfg=grey

set background=dark
